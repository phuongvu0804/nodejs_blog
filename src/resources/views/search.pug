link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous")
link(rel='stylesheet', href='/css/app.css')

header 
    nav.navbar.navbar-expand-lg.navbar-dark.bg-dark
        .container-fluid
            a.navbar-brand(href='#') Blog
            button.navbar-toggler(type='button', data-bs-toggle='collapse', data-bs-target='#navbarSupportedContent', aria-controls='navbarSupportedContent', aria-expanded='false', aria-label='Toggle navigation')
            span.navbar-toggler-icon
            #navbarSupportedContent.collapse.navbar-collapse
            ul.navbar-nav.me-auto.mb-2.mb-lg-0
                li.nav-item
                a.nav-link.active(aria-current='page', href='#') Home
                li.nav-item
                a.nav-link(href='#') Link
                li.nav-item
                a.nav-link.disabled(href='#', tabindex='-1', aria-disabled='true')
div.container
    form(method="get" action="/profile")
        .mb-3
            label.form-label(for='search-input') Từ khoá
            input#exampleInputEmail1.form-control(type='text', aria-describedby='Nhập tên', name="q")
        button.btn.btn-primary(type='submit') Tìm kiếm
    ul Danh sách
        each user in users 
            li= user.name
footer Footer